<section>
	<% if logged_in? %>
		<%= link_to 'New Forecast', user_path(current_user.id) %>
		<%= link_to 'Search History', forecasts_path(current_user.id) %>
		<%= link_to 'Logout', logout_path, method: 'delete' %>
	<% else %>
		<%= link_to 'Login', login_path %>
		<%= link_to 'Signup', new_user_path %>
	<% end %>
</section>